<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Age_validation</fullName>
    <active>true</active>
    <errorConditionFormula>Age__c &lt; 15</errorConditionFormula>
    <errorDisplayField>Age__c</errorDisplayField>
    <errorMessage>Age Should be greater than 15</errorMessage>
</ValidationRule>
